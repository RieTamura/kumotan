{
  "expo": {
    "name": "くもたん",
    "slug": "kumotan",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1DA1F2"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.kumotan.app",
      "buildNumber": "2",
      "infoPlist": {
        "CFBundleDisplayName": "くもたん",
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "学習進捗の画像を保存・シェアするために写真ライブラリへのアクセスが必要です。",
        "NSCameraUsageDescription": "投稿に添付する写真を撮影するためにカメラへのアクセスが必要です。"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1DA1F2"
      },
      "package": "com.kumotan.app"
    },
    "web": {},
    "newArchEnabled": false,
    "plugins": [
      "expo-secure-store",
      "expo-sqlite",
      "expo-asset",
      "expo-system-ui",
      [
        "expo-image-picker",
        {
          "photosPermission": "投稿に添付する画像を選択するために写真ライブラリへのアクセスが必要です。",
          "cameraPermission": "投稿に添付する写真を撮影するためにカメラへのアクセスが必要です。"
        }
      ]
    ],
    "scheme": "io.github.rietamura",
    "extra": {
      "eas": {
        "projectId": "c9480379-b17d-4c68-ac12-b4922290cf74"
      },
      "oauth": {
        "clientId": "https://rietamura.github.io/kumotan/oauth-client-metadata.json",
        "redirectUri": "io.github.rietamura:/oauth/callback"
      }
    },
    "owner": "rietamura"
  }
}