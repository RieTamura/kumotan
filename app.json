{
  "expo": {
    "name": "くもたん",
    "slug": "kumotan",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1DA1F2"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.kumotan.app",
      "buildNumber": "2",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "CFBundleDisplayName": "くもたん",
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "学習進捗の画像を保存・シェアするために写真ライブラリへのアクセスが必要です。"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1DA1F2"
      },
      "package": "com.kumotan.app"
    },
    "web": {},
    "newArchEnabled": true,
    "plugins": [
      "expo-secure-store",
      "expo-sqlite",
      "expo-build-properties"
    ],
    "scheme": "io.github.rietamura",
    "extra": {
      "eas": {
        "projectId": "c9480379-b17d-4c68-ac12-b4922290cf74"
      },
      "oauth": {
        "clientId": "https://rietamura.github.io/kumotan/oauth-client-metadata.json",
        "redirectUri": "io.github.rietamura:/"
      }
    },
    "owner": "rietamura"
  }
}