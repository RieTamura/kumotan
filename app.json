{
  "expo": {
    "name": "くもたん",
    "slug": "kumotan",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1DA1F2"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.kumotan.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "CFBundleDisplayName": "くもたん"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1DA1F2"
      },
      "package": "com.kumotan.app"
    },
    "web": {},
    "plugins": [
      "expo-secure-store",
      "expo-sqlite"
    ],
    "scheme": "kumotan",
    "extra": {
      "eas": {
        "projectId": "c9480379-b17d-4c68-ac12-b4922290cf74"
      }
    },
    "owner": "rietamura"
  }
}
