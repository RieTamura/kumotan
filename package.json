{
  "name": "kumotan",
  "version": "1.0.0",
  "description": "Blueskyで英語学習！タイムラインから単語を保存して、あなただけの単語帳を作ろう",
  "author": "RieTamura",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/RieTamura/kumotan.git"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "@atproto/oauth-client-expo"
        ]
      }
    },
    "install": {
      "exclude": [
        "react-native-reanimated"
      ]
    }
  },
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "jmdict:download": "npx ts-node scripts/jmdict/download-jmdict.ts",
    "jmdict:convert": "npx ts-node scripts/jmdict/convert-to-sqlite.ts",
    "jmdict:setup": "npm run jmdict:download && npm run jmdict:convert"
  },
  "dependencies": {
    "@atproto/api": "^0.13.0",
    "@atproto/oauth-client": "^0.5.13",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.3.13",
    "@react-navigation/native": "^7.1.9",
    "@react-navigation/native-stack": "^7.3.14",
    "@types/elliptic": "^6.4.18",
    "@types/uuid": "^10.0.0",
    "buffer": "^6.0.3",
    "core-js": "^3.47.0",
    "date-fns": "^3.6.0",
    "elliptic": "^6.6.1",
    "event-target-polyfill": "^0.0.4",
    "expo": "~54.0.31",
    "expo-asset": "~12.0.12",
    "expo-clipboard": "~8.0.8",
    "expo-crypto": "^15.0.8",
    "expo-dev-client": "^6.0.20",
    "expo-file-system": "~19.0.21",
    "expo-linking": "^8.0.11",
    "expo-secure-store": "~15.0.8",
    "expo-sharing": "~14.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-sqlite": "~16.0.10",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "^15.0.10",
    "fflate": "^0.8.2",
    "i18next": "^25.7.4",
    "lucide-react-native": "^0.562.0",
    "react": "19.1.0",
    "react-i18next": "^16.5.3",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-image-viewing": "^0.2.2",
    "react-native-pager-view": "6.9.1",
    "react-native-reanimated": "~3.17.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-view-shot": "4.0.3",
    "uuid": "^13.0.0",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.28.5",
    "@types/better-sqlite3": "^7.6.8",
    "@types/jest": "^29.5.0",
    "@types/react": "~19.1.10",
    "@types/react-test-renderer": "^19.1.0",
    "babel-jest": "^30.2.0",
    "babel-preset-expo": "^54.0.9",
    "better-sqlite3": "^11.0.0",
    "eslint": "^9.0.0",
    "eslint-config-expo": "~10.0.0",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.16",
    "react-test-renderer": "^19.1.0",
    "ts-jest": "^29.1.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
